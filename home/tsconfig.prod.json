{
  "compilerOptions": {
    "jsx": "react-jsx",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./src/app",
    "declaration": true,
    "emitDeclarationOnly": true,
    "isolatedModules": true,
    "outDir": "lib",
    "rootDir": "src/app",
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "include": [
    "src/app"
  ],
  "exclude": [
    "node_modules",
  ]
}
